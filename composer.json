{
    "name": "bramato/laravel-selling-partner-amazon",
    "description": ":package_description",
    "keywords": [
        ":vendor_name",
        "laravel-selling-partner-amazon"
    ],
    "homepage": "https://github.com/bramato/laravel-selling-partner-amazon",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Bramato",
            "email": "marco@bramato.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "bramatom/selling-partner-amazon": "^0.1.2",
        "illuminate/support": "^9.46"
    },
    "require-dev": {
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Bramatom\\LaravelSellingPartnerAmazon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bramatom\\LaravelSellingPartnerAmazon\\Tests\\": "tests"
        }
    },
    "scripts": {

    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bramatom\\LaravelSellingPartnerAmazon\\AmazonSellingPartnerServiceProvider"
            ],
            "aliases": {
                "AmazonSellingPartner": "Bramatom\\LaravelSellingPartnerAmazon\\Facades\\AmazonSellingPartner"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
